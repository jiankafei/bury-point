{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "ENV": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
    "allowImportExportEverywhere": true
  },
  "rules": {
    "no-unused-vars": 0,
    "no-console": 0,
    "no-extra-semi": 0
  }
}
