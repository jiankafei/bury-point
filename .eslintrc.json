{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "ENV": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "allowImportExportEverywhere": true,
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "rules": {
    "no-unused-vars": 0,
    "no-console": 0,
    "no-extra-semi": 0,
    "consistent-return": 0
  }
}
